{
  "name": "epic-games-deck",
  "version": "0.0.1",
  "description": "Manage epic games on the steam deck.",
  "scripts": {
    "prepare": "pip3 install --upgrade -r requirements.txt -t py_modules",
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatter/epic-games-deck.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "deck",
    "epic"
  ],
  "author": "Ava Johnson <ava.johnson@zohomail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/RedHatter/epic-games-deck/issues"
  },
  "homepage": "https://github.com/RedHatter/epic-games-deck#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "prettier": "^3.1.1",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.24.1",
    "react-icons": "^4.4.0",
    "remeda": "^1.29.0",
    "zustand": "^4.4.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
